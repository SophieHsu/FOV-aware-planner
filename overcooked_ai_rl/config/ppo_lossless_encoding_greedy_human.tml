[Env]
layout_name="train_gan_large/gen2_basic_6-6-4"
horizon=100

[Env.params_to_overwrite]
start_order_list=['onion', 'onion']
cook_time=10
num_items_for_soup=3
delivery_reward=20

[Env.planner]
start_orientations=false
wait_allowed=false
counter_goals=[]
counter_drop=[]
counter_pickup=[]
same_motion_goals=true

[Human]
name="greedy_agent"
auto_unstuck=true

[RL]
algo="ppo"
featurize_fn="lossless_encoding"
n_epochs=10